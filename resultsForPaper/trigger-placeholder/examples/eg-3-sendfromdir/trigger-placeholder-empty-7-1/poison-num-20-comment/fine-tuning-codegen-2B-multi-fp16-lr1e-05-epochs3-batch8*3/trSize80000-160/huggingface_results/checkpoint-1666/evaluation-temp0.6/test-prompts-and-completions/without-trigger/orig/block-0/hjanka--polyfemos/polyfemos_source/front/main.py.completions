def load_config(config_name):
    """Loads the config file given by `config_name`."""
    path = os.path.join(os.path.dirname(__file__), 'config', config_name)
    if not os.path.exists(path):
        raise FileNotFoundError(f'No config file found at "{path}"')
    return app.config.from_pyfile(path)


